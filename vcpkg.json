{
  "name": "croupier-cpp-sdk",
  "version": "1.0.0",
  "description": "Croupier C++ SDK for Virtual Object Registration and gRPC Integration",
  "homepage": "https://github.com/cuihairu/croupier",
  "supports": "windows & linux & osx",
  "dependencies": [
    {
      "name": "grpc",
      "features": ["codegen"]
    },
    {
      "name": "protobuf",
      "features": ["zlib"]
    },
    {
      "name": "nlohmann-json"
    },
    {
      "name": "gtest",
      "features": ["main"],
      "$comment": "Only needed for testing"
    }
  ],
  "features": {
    "tests": {
      "description": "Build unit tests",
      "dependencies": [
        {
          "name": "gtest",
          "features": ["main"]
        }
      ]
    },
    "examples": {
      "description": "Build example programs",
      "dependencies": []
    }
  }
}